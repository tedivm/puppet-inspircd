
<config format="xml">

<server
        name="penguin.omega.example.org"
        description="Waddle World"
        network="Omega">


<admin
       name="Johnny English"
       nick="MI5"
       email="MI5@the.best.secret.agent">


<power
       #hash="sha256"
       diepass=""
       restartpass="">

<cidr
      ipv4clone="32"
      ipv6clone="128">


# This file has all the information about oper classes, types and o:lines.
# You *MUST* edit it.
<include file="conf/examples/opers.conf.example">

# This file has all the information about server links and ulined servers.
# You *MUST* edit it if you intend to link servers.
<include file="conf/examples/links.conf.example">



<files motd="conf/examples/motd.txt.example" rules="conf/examples/rules.txt.example">

# Example of an executable file include. Note this will be read on rehash,
# not when the command is run.
#<execfiles rules="wget -O - http://www.example.com/rules.txt">



<channels
          users="20"
          opers="60">

#-#-#-#-#-#-#-#-#-#-#-#-#-#-# DNS SERVER -#-#-#-#-#-#-#-#-#-#-#-#-#-#-#
# If these values are not defined, InspIRCd uses the default DNS resolver
# of your system.

<dns
     # server="127.0.0.1"
     timeout="5">

# An example of using an IPv6 nameserver
#<dns server="::1" timeout="5">

#<pid file="/path/to/inspircd.pid">
#<disabled commands="TOPIC MODE" usermodes="" chanmodes="" fakenonexistant="yes">

<options
         prefixquit="Quit: "
         suffixquit=""
         prefixpart="&quot;"
         suffixpart="&quot;"

         #fixedquit=""
         #fixedpart=""
         syntaxhints="no"
         cyclehosts="yes"
         cyclehostsfromuser="no"

         ircumsgprefix="no"
         announcets="yes"
         allowmismatch="no"
         defaultbind="auto"
         hostintopic="yes"
         pingwarning="15"
         serverpingfreq="60"
         defaultmodes="nt"
         moronbanner="You're banned! Email abuse@example.com with the ERROR line below for help."
         exemptchanops="nonick:v flood:o"
         invitebypassmodes="yes"
         nosnoticestack="no"
         welcomenotice="yes">



<performance
             netbuffersize="10240"
             somaxconn="128"
             limitsomaxconn="true"
             softlimit="12800"
             quietbursts="yes"
             nouserdns="no">


<security

          announceinvites="dynamic"
          hidemodes="eI"
          hideulines="no"
          flatlinks="no"
          hidewhois=""
          hidebans="no"
          hidekills=""
          hidesplits="no"
          maxtargets="20"
          customversion=""
          operspywhois="no"
          #runasuser=""
          #runasgroup=""
          restrictbannedusers="yes"
          genericoper="no"
          userstats="Pu">


<limits
        maxnick="31"
        maxchan="64"
        maxmodes="20"
        maxident="11"
        maxquit="255"
        maxtopic="307"
        maxkick="255"
        maxgecos="128"
        maxaway="200">



<log method="file" type="* -USERINPUT -USEROUTPUT" level="default" target="logs/ircd.log">

<whowas
        groupsize="10"
        maxgroups="100000"
        maxkeep="3d">

<insane
        hostmasks="no"
        ipmasks="no"
        nickmasks="no"
        trigger="95.5">
